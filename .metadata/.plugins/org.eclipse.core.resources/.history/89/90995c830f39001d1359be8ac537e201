package questThree;

import java.util.LinkedList;
import java.util.List;

public class ArrayB {

	public static void main(String[] args) {
		List<Pickle> list = new LinkedList<Pickle>();
		list.add(new Pickle("Dill"));
		list.add(new Pickle("Sweet"));
		
		System.out.println(list.get(1).flavor);
		System.out.println(list.contains(new Pickle("Sweet")));
	}

}

class Pickle {
	String flavor;
	public Pickle(String flavor) {
		this.flavor = flavor;
	}
}